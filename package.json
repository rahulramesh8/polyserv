{
  "name": "polyserv",
  "version": "0.0.1",
  "description": "Map bounds to Polygons",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "yarn run -s build",
    "test": "eslint src",
    "build-db": "docker build -t polyserv-db ./postgres",
    "run-db": "docker run -v db-data:/var/lib/postgresql/data -p 5432:5432 --env-file postgres/.env polyserv-db"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "https://github.com/KCRR/polyservapi",
  "author": "KCRR",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "6.0.0",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "pg": "7.4.3",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.2",
    "babel-eslint": "8.2.6",
    "eslint": "4.2.0",
    "eslint-config-airbnb": "15.0.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.1.0",
    "prettier": "1.12.1"
  }
}
